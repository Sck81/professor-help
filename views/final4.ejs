<!DOCTYPE html>
<html>
    <head>
         <link href="final.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
        <title>ProfessorHelp</title>
    </head>
    <body>
        <% include header %>
        <%if (items.length >0) { %>
            <h1 class="major1"><%= items[0].FullName %></h1>
        <% } %>
            <div class="review">
                <a href=final5.html><button class="bt-review">Write a Review</button></a>  
            </div>
        <h3 class="box1"> Overall </h3>
            <div class="rate">
                4.5 Good!
            </div>
        <h3 class="box1"> Reviews </h3>   
        <% items.forEach(function (item) { %>
            <div class="reviewWrite">
                <div class="reviewHeader">
                    <div class="reviewerName"> <%= item.FirstName %> <%= item.LastName %> </div>
                </div>
                <div class="review"><%= item.Comment %> </div>
            </div> 
        <% }) %> 
    </body>
</html>

